{
    "name": "lightbase/cocar",
    "version": "1.0",
    "license": "CC-GPL 2.0",
    "type": "project",
    "description": "COCAR",
    "autoload": {
	"psr-0": { "Swpb\\Bundle\\CocarBundle": "" }
    },
    "target-dir": "Swpb/Bundle/CocarBundle",
    "require": {
        "php": ">=5.3.2",
	"jpgraph/jpgraph": "dev-master",
	"ddeboer/data-import-bundle": "dev-master"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
